<!doctype html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport"
        content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
  <meta http-equiv="X-UA-Compatible" content="ie=edge">
  <title>Document</title>
  <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/css/bootstrap.min.css"
        integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
</head>
<body>

<div class="container">
  <form style="margin-top: 100px" method="post" action="/addFood">

    <div class="form-group">
      <input name="name" type="text" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp"
             placeholder="Name Food">

    </div>
    <div class="form-group">
      <input name="urlImage" type="url" class="form-control" id="exampleInputPassword1" placeholder="Url Image Food">
    </div>
    <div class="form-group">
      <input name="price" type="text" class="form-control" id="exampleInputPassword1" placeholder="Price">
    </div>
    <div class="form-group">
      <input name="total" type="number" class="form-control" id="exampleInputPassword1" placeholder="Total">
    </div>
    <div class="form-group">
      <input name="type" type="number" class="form-control" id="exampleInputPassword1" placeholder="Type">
    </div>

    <button style="text-align: center" type="submit" class="btn btn-info">Add</button>
  </form>


</div>
<script>
  function check() {
    var url = document.getElementById('exampleInputEmail1').value;
    if (url == "") {
      alert("Please enter something");
    }
  }
</script>
</body>
</html>
